{
  "name": "redux-observable",
  "version": "3.0.0-rc.2",
  "type": "module",
  "description": "RxJS based middleware for Redux. Compose and cancel async actions and more.",
  "main": "dist/redux-observable.cjs",
  "module": "dist/redux-observable.mjs",
  "types": "types/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/redux-observable.mjs",
      "require": "./dist/redux-observable.cjs",
      "default": "./dist/redux-observable.cjs"
    }
  },
  "sideEffects": false,
  "scripts": {
    "lint": "eslint src test --ext .ts",
    "lint:fix": "eslint src test --ext .ts --fix",
    "build": "tsup",
    "prebuild": "tsc --emitDeclarationOnly",
    "prepare": "npm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "analyze": "source-map-explorer \"dist/*.{mjs,cjs}\""
  },
  "files": [
    "dist",
    "types",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/redux-observable/redux-observable.git"
  },
  "keywords": [
    "Rx",
    "Ducks",
    "Reducks",
    "Redux",
    "middleware",
    "observable",
    "thunk",
    "async",
    "cancel",
    "action"
  ],
  "contributors": [
    {
      "name": "Ben Lesh",
      "email": "ben@benlesh.com"
    },
    {
      "name": "Jay Phelps",
      "email": "hello@jayphelps.com"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/redux-observable/redux-observable/issues"
  },
  "homepage": "https://github.com/redux-observable/redux-observable#README.md",
  "engines": {
    "node": ">=18.17.0"
  },
  "peerDependencies": {
    "redux": ">=5",
    "rxjs": ">=7.8.0"
  },
  "devDependencies": {
    "@types/node": "^20.19.11",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.0.0",
    "redux": "^5.0.0",
    "rxjs": "^7.8.2",
    "tsup": "^8.0.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.0.0",
    "vitest": "^3.2.4",
    "source-map-explorer": "^2.5.3"
  }
}
